<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <ImageButton
            android:background="@drawable/ic_border_color_red_500_24dp"
            android:layout_width="30dp"
            android:layout_height="30dp"
            tools:ignore="MissingConstraints"
            android:id="@+id/ib_mbst"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@mipmap/ic_launcher"
            android:id="@+id/imageView"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbname"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_status"
            android:textSize="30sp" android:textStyle="bold"/>
    <TextView
            android:text="Status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_status"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:textSize="18sp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/tv_mbname"
            android:fitsSystemWindows="false"/>
    <TextView
            android:text="아이디 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbId"
            tools:ignore="MissingConstraints"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="25sp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9"/>
    <TextView
            android:text="회원 ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbID"
            app:layout_constraintBottom_toBottomOf="@+id/tv_mbId"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/tv_mbId"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="이메일 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:textSize="25sp"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_mbId"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="20dp"/>
    <TextView
            android:text="국적   :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:textSize="25sp"
            android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="직업   :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:textSize="25sp"
            android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="성별   :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            android:textSize="25sp"
            android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="회원 이메일"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbmail"
            android:textSize="25sp"
            tools:ignore="MissingConstraints"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_marginStart="28dp"/>
    <TextView
            android:text="회원 국적"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbnt"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="28dp"/>
    <TextView
            android:text="회원 직업"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbjob"
            android:textSize="25sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_marginStart="28dp"/>
    <TextView
            android:text="이름   :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="112dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:textSize="25sp"
            android:textAlignment="viewStart"/>
    <TextView
            android:text="회원이름"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView9"
            android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            android:layout_marginStart="28dp"/>
    <RadioGroup
            app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fitsSystemWindows="false"
            tools:ignore="MissingConstraints"
            android:id="@+id/radioGroup"
            tools:layout_editor_absoluteX="307dp"/>
    <RadioGroup
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_marginStart="28dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView5">
        <RadioButton
                android:textSize="25dp"
                android:text="남"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/rb_man"
                android:layout_weight="1"/>
        <RadioButton
                android:textSize="25dp"
                android:text="여"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/rb_woman"
                android:layout_weight="1"/>
    </RadioGroup>
    <Button
            android:text="LogOut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/bt_mbout"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/ib_mbst"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tv_mbname"
            android:layout_marginStart="120dp"/>
</android.support.constraint.ConstraintLayout>