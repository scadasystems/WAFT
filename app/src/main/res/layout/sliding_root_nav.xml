<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <ImageView
            android:background="@drawable/ic_border_color_red_500_24dp"
            android:layout_width="45dp"
            android:layout_height="45dp"
            tools:ignore="MissingConstraints"
            android:id="@+id/imageView2"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView" android:layout_marginEnd="20dp"
            app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:background="@mipmap/ic_launcher"
            android:id="@+id/imageView"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbname"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/tv_status" android:textSize="30sp"/>
    <TextView
            android:text="Status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_status" app:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:textSize="18sp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/tv_mbname"/>
    <TextView
            android:text="아이디 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_mbId" tools:ignore="MissingConstraints"
            android:layout_marginStart="20dp" app:layout_constraintStart_toStartOf="parent" android:textSize="25sp"
            android:layout_marginTop="30dp" app:layout_constraintTop_toBottomOf="@+id/textView9"/>
    <TextView
            android:text="회원 ID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintBottom_toBottomOf="@+id/tv_mbId"
            android:textSize="25sp" app:layout_constraintStart_toEndOf="@+id/tv_mbId"
            android:layout_marginStart="17dp"/>
    <TextView
            android:text="이메일 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:textSize="25sp" android:layout_marginTop="30dp" app:layout_constraintTop_toBottomOf="@+id/tv_mbId"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="20dp"/>
    <TextView
            android:text="국적 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" android:textSize="25sp"
            android:layout_marginStart="30dp" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="직업 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:textSize="25sp"
            android:layout_marginStart="30dp" app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="성별 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" android:textSize="25sp"
            android:layout_marginStart="30dp" app:layout_constraintStart_toStartOf="parent"/>
    <RadioButton
            android:text="남"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:layout_marginStart="30dp" app:layout_constraintBottom_toBottomOf="@+id/textView5"
            android:textSize="25sp"/>
    <RadioButton
            android:text="여"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton2" app:layout_constraintStart_toEndOf="@+id/radioButton"
            app:layout_constraintBottom_toBottomOf="@+id/radioButton"
            app:layout_constraintTop_toTopOf="@+id/radioButton" android:layout_marginStart="15dp"
            android:textSize="25sp"/>
    <TextView
            android:text="회원 이메일"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
            android:textSize="25sp" app:layout_constraintStart_toEndOf="@+id/textView2"
            android:layout_marginStart="17dp"/>
    <TextView
            android:text="회원 국적"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView3"
            android:textSize="25sp" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="30dp"/>
    <TextView
            android:text="회원 직업"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:textSize="25sp" app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginStart="30dp"/>
    <TextView
            android:text="이름 :"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView9"
            android:layout_marginTop="120dp" app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="30dp" android:textSize="25sp"
            android:textAlignment="viewStart"/>
    <TextView
            android:text="회원이름"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView9" android:textSize="25sp"
            app:layout_constraintStart_toEndOf="@+id/textView9" android:layout_marginStart="30dp"/>
</android.support.constraint.ConstraintLayout>